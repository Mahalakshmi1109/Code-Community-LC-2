bool isPowerOfFour(int n) {
     long long int arr[50];
    int f=0;
    for(int i=0;i<50;i++)
    {
        arr[i]=pow(4,i);
        if(n==arr[i])
    {
        f=1;
        break;
    }
    }
    if(f==1)
    {
        return true;
    }
    else 
    {
        return false;
    }
    
        
    }

    

